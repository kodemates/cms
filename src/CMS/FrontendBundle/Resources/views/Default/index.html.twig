{% extends '::base.html.twig' %}

{% block body %}
    {% for content in contents %}
        <div>
            <h3>
                <a href="{{ path('cms_frontend_content_show', {id: content.id}) }}">{{ content.title }}</a>
            </h3>
	    <div class="content">
                {{ content.body[:100] }}
            </div>

        </div>
    {% endfor %}
{% endblock %}
